def main():

    person_info = {
        "name": "John Doe",
        "age": 30,
        "gender": "Male",
        "city": "New York"
    }

    print("Person Information Dictionary:")
    for key, value in person_info.items():
        print(f"{key}: {value}")
    

    dictionary_length = len(person_info)
    print(f"\nLength of the dictionary: {dictionary_length}")

if __name__ == "__main__":
    main()
